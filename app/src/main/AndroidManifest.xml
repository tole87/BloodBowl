<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.miguel.bludbuwl">

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GestionEquipoMenu"
            android:label="@string/title_activity_gestion_equipo_menu"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GuiaEquipos"
            android:label="@string/title_activity_guia_equipos"
            android:screenOrientation="landscape" />
        <activity
            android:name=".CreacionEquiposLista"
            android:label="@string/title_activity_creacion_equipos_lista"></activity>
    </application>

</manifest>